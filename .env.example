# Token da API do Telegram (obrigatório)
# Obtenha com o BotFather: https://t.me/botfather
TELEGRAM_API_TOKEN=seu_token_aqui

# ID do proprietário do bot (obrigatório)
# Este é o seu ID de usuário do Telegram
# Você pode obtê-lo enviando uma mensagem para @userinfobot
OWNER_ID=seu_id_aqui

# Nome de usuário do bot (opcional, padrão: Nations_bro_bot)
# Não inclua o símbolo @ no início
BOT_USERNAME=Nations_bro_bot

# Nível de log (opcional, padrão: INFO)
# Opções: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Mensagem de boas-vindas personalizada (opcional)
# Se não for definida, será usada a mensagem padrão
# WELCOME_MESSAGE=Sua mensagem personalizada aqui 